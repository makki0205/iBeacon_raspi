
git clone git://github.com/creationix/nvm.git ~/.nvm
echo 'export NVM_DIR="$HOME/.nvm"' >> .bashrc
echo '[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh" # This loads nvm'>> .bashrc
source .bashrc
nvm install v0.10.33
sudo apt-get update
sudo apt-get install libbluetooth-dev
npm install bleacon
sudo node beacon.js
